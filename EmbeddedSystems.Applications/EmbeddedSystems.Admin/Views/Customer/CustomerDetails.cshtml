@model EmbeddedSystems.DomainModel.Customer

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <td>Name</td>
            <td>Email</td>
            <td>Address</td>
            <td>Contact No.</td>
            <td>Knowledge Level</td>
            <td>Language</td>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Name</td>
            <td>@Model.Email</td>
            <td>@Model.Address</td>
            <td>@Model.MobileNumber</td>
            <td>@Model.KnowledgeLevel</td>
            <td>@Model.Language</td>
        </tr>
    </tbody>
    <tfoot></tfoot>
</table>
